@{
    ViewData["Title"] = "Welcome on board, you are sailing with Linux today";
}
@model IEnumerable<LinuxDotnet.Models.Customer>
<div id="body">
    <img src="~/images/bodybg.jpeg" />
    <h2>@ViewData["Title"]</h2>
    <table class="table">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Age</th>
        </tr>

        @foreach (var item in Model)
        {
        <tr>
            <td>
                @item.FirstName
            </td>
            <td>
                @item.LastName
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @item.Age
            </td>
        </tr>
        }
    </table>
</div>

